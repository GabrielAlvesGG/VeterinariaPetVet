@model Cliente

<h1>Editar Cadastro </h1>

<form method="POST" asp-action="AtualizarCliente">
    <input type="hidden" asp-for="id">
    <p>
        <label asp-for="nome">Nome:</label>
        <input type="text" asp-for="nome" placeholder="nome" required>
    </p>
    <p>
        <label asp-for="dataNascimento">Data Nascimento:</label>
        <input type="date" asp-for="dataNascimento" required>
    </p>
    <p>
        <label asp-for="login">Login:</label>
        <input type="text" asp-for="login" placeholder="Login" required>
    </p>
    <p>
        <label asp-for="senha">Senha:</label>
        <input type="password" asp-for="senha" placeholder="Senha">
    </p>
    <p>
    <input type="submit" value="Editar">
    </p>
</form>
